<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aloos.finalproject2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/ss"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activites.Splash"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.LogInActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".activites.Signup"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_signup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:screenOrientation="portrait"

            android:name=".activites.NavDoctor"
            android:label="@string/title_activity_nav_doctor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:screenOrientation="portrait"

            android:name=".activites.NavStudent"
            android:label="@string/title_activity_nav_student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:screenOrientation="portrait"

            android:name=".activites.QuastionType"
            android:label="@string/title_activity_quastion_type"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:screenOrientation="portrait"

            android:name=".activites.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activites.SolveQuiz"
            android:screenOrientation="portrait"


            />
        <activity android:name=".activites.Startquiz"
            android:screenOrientation="portrait"

            />
        <activity android:name=".activites.FinalScore"
            android:screenOrientation="portrait"

            />
        <activity android:name=".activites.Showquestions"
            android:screenOrientation="portrait"

            />
        <activity android:name=".activites.Mark"
            android:screenOrientation="portrait"

            ></activity>
    </application>

</manifest>